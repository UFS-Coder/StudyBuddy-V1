{
  "functions": {
    "api/generate-practice-questions.js": {
      "runtime": "nodejs18.x"
    },
    "api/groq-chat.js": {
      "runtime": "nodejs18.x"
    },
    "api/groq-facts.js": {
      "runtime": "nodejs18.x"
    }
  },
  "rewrites": [
    {
      "source": "/((?!api).*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}